# set beams
RG:Beam1 = %beam1%
RG:Beam2 = %beam2%
RG:Mom1 = %beamEn%
RG:Mom2 = %beamEn%
RG:Seed = %seed%
RG:NumEvents = %nevts%


#Top Pair production (inclusive)
MSEL 6


## Set cuts
## Use this for hard leading-jets in a certain pT window
## min pT
#CKIN(3) %pTmin%
## max pT
#CKIN(4) %pTmax%
#
## Use this for hard leading-jets in a certain mHat window
## min mHat
#CKIN(1) %mHatMin%
## max mHat
#CKIN(2) %mHatMax%


# Switch off hadronization
MSTP(111) 0

# Set the top stable
MDCY(6,1) 0

# Makes particles with c*tau > 10 mm stable:
MSTJ(22)     2
PARJ(71)     10.0

# set tune
MSTP(5) %tuneCode%

